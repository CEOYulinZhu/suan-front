.levelcontainer {
    position: relative;
  }
  
  .level-select-guide-achievement {
    padding: 20rpx;
  }

  .item.addition {
    background-color: #e6f7ff; /* 加法关卡背景色，可根据需求调整 */
  }
  .item.subtraction {
    background-color: #f0fff0; /* 减法关卡背景色，可根据需求调整 */
  }
  .item.multiplication {
    background-color: #fff3e0; /* 乘法关卡背景色，可根据需求调整 */
  }
  .item.mixed {
    background-color: #f9e6ff; /* 混合运算关卡背景色，可根据需求调整 */
  }
  .star {
    
    width: 25px;
    height: 25px;
    display: flex;
    justify-content: center;
    align-items: center;
    color: white;
  }
  .levelmodeicons {
    
    height: 25rpx;
    width: 25rpx;
    filter: invert(100%) grayscale(100%);
}
.circleonestarandtwo {
    margin-right: 10px;
    width: 50rpx;      /* 设置宽度 */
    height: 50rpx;     /* 高度必须与宽度相同 */
    border-radius: 50%; /* 将边框半径设为50%创建圆形 */
    background-color: #e76d09; /* 背景颜色 */
  }
  .circlethreestar {
    margin-right: 10px;
    width: 50rpx;      /* 设置宽度 */
    height: 50rpx;     /* 高度必须与宽度相同 */
    border-radius: 50%; /* 将边框半径设为50%创建圆形 */
    background-color: rgb(255, 203, 69); /* 背景颜色 */
  }
  .circlelock{
    margin-right: 10px;
    width: 50rpx;      /* 设置宽度 */
    height: 50rpx;     /* 高度必须与宽度相同 */
    border-radius: 50%; /* 将边框半径设为50%创建圆形 */
    background-color: rgb(22, 119, 255); /* 背景颜色 */
  }
  .level-select {
    background: #fff;
    border-radius: 15rpx;
    padding: 20rpx;
    
    margin-bottom: 20rpx;
  }
  
  .header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 15rpx;
    color: rgb(0, 0, 0);
  }
  
  .passed {
    color: #666;
    font-size: 28rpx;
  }
  
  .progress {
    color: #999;
    font-size: 28rpx;
    margin-bottom: 15rpx;
    display: flex;
    justify-content: center;
    align-items: center;
  }
  
  .grid {
    display: grid;
    grid-template-columns: repeat(4, 1fr);
    gap: 15rpx;
  }
  
  .item {
    border: 1rpx solid #ddd;
    border-radius: 10rpx;
    padding: 25rpx;
    text-align: center;
  }
  
  .item.locked {
    background: #ccc; 
    opacity: 0.6;
    pointer-events: none;
  }
  
  .item.passed {
    background: #f5f5f5;
    opacity: 1;
  }
  
  .num {
    font-size: 40rpx;
    font-weight: bold;
    margin-bottom: 10rpx;
  }
  
  .type {
    color: #666;
    font-size: 28rpx;
  }
  
  .guide {
    background: #fff;
    border-radius: 15rpx;
    padding: 20rpx;
    margin-bottom: 20rpx;
  }
  
  .guide-item {
    display: flex;
    padding: 20rpx;
    background-color: rgb(250, 250, 250);
    align-items: center;
    margin-bottom: 25rpx;
    font-size: 28rpx;
  }
  
  .achievement {
    background: #fff;
    border-radius: 15rpx;
    padding: 20rpx;
    margin-bottom: 20rpx;
  }
  .question{
    
    background: #fff;
    border-radius: 15rpx;
    padding: 20rpx;
    margin-bottom: 20rpx;
  }
  .achievement-item {
    display: flex;
    align-items: center;
    margin-bottom: 15rpx;
    font-size: 28rpx;
  }
  
  .progress-tag {
    background: #e0f0ff;
    color: #007AFF;
    padding: 5rpx 15rpx;
    border-radius: 20rpx;
    margin-left: 15rpx;
  }
  
  .answer-wrap {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    padding: 30rpx;
    background: #fff;
    display: flex;
    flex-direction: column;
    
    z-index: 999;
  }
  
  .progresst-bar {
    width: 100%;
    height: 8rpx;
    background: #adadaf;
    margin-bottom: 20rpx;
  }
  
  .progress-fill {
    
    height: 100%;
    background: #007AFF;
  }
  
  .math-question {
    font-size: 50rpx;
    margin: 30rpx 0;
  }
  
  .inputAnswer {
    border: 1rpx solid #ddd;
    padding: 15rpx;
    border-radius: 8rpx;
    margin-bottom: 20rpx;
    width: 80%;
    margin: 0 auto 20rpx;
  }
  
  .btn-group {
    display: flex;
    gap: 20rpx;
    margin-bottom: 30rpx;
    justify-content: center;
  }
  
  .progress-summary {
    
    justify-content: center;
    padding: 10rpx;
    font-size: 30rpx;
    color: #666;
  }
  
  .result-wrap {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    display: flex;
    flex-direction: column;
    
    align-items: center;
    background: #fff;
    z-index: 999;
    padding: 20rpx;
  }
  
  .replay-btn,.back-btn {
    width: 500rpx;
    padding: 20rpx;
    border: none;
    border-radius: 20rpx;
    font-size: 32rpx;
    justify-content: center;
    align-items: center;
    display: flex;
    background-color: rgb(86, 161, 231);
  }
  
  .wrong-list {
    width: 100%;
    margin-top: 20rpx;
  }
  
  .wrong-item {
    margin: 15rpx 0;
    color: #ff3b30;
    font-size: 32rpx;
  }
  
  .modal {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    background: rgba(0, 0, 0, 0.5);
  }
  
  .modal-content {
    background: white;
    border-radius: 20rpx;
    padding: 30rpx;
    text-align: center;
    
  }
  
  .title {
    font-size: 36rpx;
    margin-bottom: 20rpx;
  }
  
  .info {
    display: flex;
    align-items: center;
    justify-content: flex-start;
    margin-bottom: 20rpx;
    font-size: 32rpx;
  }
  
  .info text:first-child {
    color: #666;
    margin-right: 15rpx;
  }
  
  .blue {
    color: #007AFF;
  }
  
  .btn-group {
    display: flex;
    gap: 20rpx;
    margin-top: 20rpx;
  }
  
  .start-btn,.submit-btn,.confirm-btn,.claim-btn {
    background: #007AFF;
    color: white;
    padding: 15rpx 40rpx;
    border-radius: 8rpx;
    font-size: 32rpx;
  }
  
  .cancel-btn,.giveup-btn,.back-btn {
    background: #fff;
    color: #666;
    padding: 15rpx 40rpx;
    border: 1rpx solid #ddd;
    border-radius: 8rpx;
    font-size: 32rpx;
  }
  
  /* 放弃确认弹窗遮罩层样式 */
  .giveup-confirm-modal-container {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.5);
    z-index: 1000;
    display: flex;
    align-items: center;
    justify-content: center;
  }
  
  .giveup-confirm-modal {
    background: white;
    border-radius: 10rpx;
    padding: 20rpx;
    text-align: center;
  }
  
  .modal-title {
    font-size: 32rpx;
    margin-bottom: 15rpx;
  }
  
  .modal-btns {
    display: flex;
    justify-content: center;
    gap: 15rpx;
    margin-top: 15rpx;
  }
  
  .confirm-btn {
    background: #007AFF;
    color: white;
    padding: 12rpx 40rpx;
    border-radius: 6rpx;
  }
  
  .cancel-btn {
    background: #fff;
    color: #666;
    padding: 12rpx 40rpx;
    border: 1rpx solid #ddd;
    border-radius: 6rpx;
  }
  
  /* 奖励弹窗遮罩层样式 */
  .reward-modal-container {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.5);
    z-index: 1000;
    display: flex;
    align-items: center;
    justify-content: center;
  }
  
  .reward-modal {
    background: white;
    border-radius: 10rpx;
    padding: 20rpx;
    text-align: center;
  }
  
  .title {
    font-size: 32rpx;
    margin-bottom: 15rpx;
  }
  
  .claim-btn {
    background: #007AFF;
    color: white;
    padding: 12rpx 40rpx;
    border-radius: 6rpx;
  }

  .item.passed::after {
    content: '\ue608'; /* 这里使用的是微信小程序默认图标库中的对勾图标代码，若不生效需确认图标库支持，也可替换为自己的图标路径 */
    position: absolute;
    top: 10rpx; /* 调整图标垂直位置 */
    right: 10rpx; /* 调整图标水平位置 */
    font-size: 20rpx; /* 调整图标大小 */
    color: inherit; /* 使图标颜色继承自父元素，即与背景色一致 */
  }

  .lock-icon {
    position: absolute;
    top: 0;
    left: 0;
    width: 50%;
    height: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
    
  }
  .lock-icon image {
    max-width: 80%;
    max-height: 80%;
  }    
  .achievement {
  padding: 30rpx;
}

.achievement-item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 25rpx;
  padding: 20rpx;
  background: #f5f5f5;
  border-radius: 15rpx;
}

.achievement-icon {
  width: 45rpx;
  height: 45rpx;
  margin-right: 20rpx;
}

.achievement-title {
  font-size: 34rpx;
  flex: 1;
  color: #333;
}

.progress-group {
  display: flex;
  align-items: center;
}

.progress-label {
  color: #007aff;
  font-size: 28rpx;
  margin-right: 15rpx;
}

.progress-bar {
  width: 220rpx;
  height: 12rpx;
  background: #e6e6e6;
  border-radius: 6rpx;
  margin-right: 15rpx;
}

.progress-fill {
  height: 100%;
  background: #007aff;
  border-radius: 6rpx;
}

.progress-text {
  font-size: 28rpx;
  color: #666;
}
.result-title{
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 15rpx;
    color: rgb(0, 0, 0);
    font-size: 20px;
}
.result-stats{
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 15rpx;
    color: rgb(107, 104, 104);
    font-size: 15px;
}
.finish-icon{
    margin: 20rpx auto 40rpx;
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    background-color: #e6f4ff;
    display: flex;
    justify-content: center;
    align-items: center;
}
/* 在WXSS文件中添加样式 */
.result-tip {
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    z-index: 1000;
}

.result-tip-content {
    padding: 15rpx 30rpx;
    border-radius: 10rpx;
    color: white;
    font-size: 32rpx;
}

.result-tip-content.correct {
    background-color: #00B42A;
}

.result-tip-content.wrong {
    background-color: #F53F3F;
}

.result-tip-content.warning {
    background-color: #1f1e1d;
}