/* 今日挑战 */
.challenge {
  background-color: white;
  padding: 20px;
  border-bottom: 1px solid #eee;
  margin-bottom: 50rpx;
  border-radius: 30px;
}

.diOOcZ {
  font-weight: bold;
  font-size: 18px;
  margin-bottom: 8px;
  color: rgb(51, 51, 51);
  display: flex;
  justify-content: center; /* 水平居中 */
}

.section-title {
  font-size: 16px;
