  gap: 8px;
}

.question {
  font-size: 28px;
  font-weight: bold;
  text-align: center;
  margin: 30px 0;
  color: #333;
}

.options {
  display: grid;
  grid-template-columns: 1fr 1fr;
  gap: 15px;
  margin-bottom: 20px;
}

.option {
  background-color: #f5f7fa;
  border-radius: 10px;
  padding: 20px;
  text-align: center;
  font-size: 20px;
  cursor: pointer;
  transition: all 0.2s;
  margin-bottom: 10px;
  border: 2px solid transparent;
}

.option:hover {
  background-color: #e6f7ff;
}

.option.correct {
  background-color: #e6ffec;
  border: 2px solid #52c41a;
  color: #007aff;
}

.option.incorrect {
  background-color: #fff2f2;
  border: 2px solid #f5222d;
  color: #ff0000;
}

/* 答题结果提示 */
.result-tip {
  margin-top: 20px;
  padding: 15px;
  border-radius: 8px;
  text-align: center;
}

.correct-tip {
  background-color: #e6ffec;
